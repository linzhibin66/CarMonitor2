<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

        <TextView
            android:id="@+id/faq_result_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/magin_26"
            android:layout_marginRight="@dimen/magin_5"
            android:textSize="14sp"
            android:layout_marginBottom="@dimen/magin_16"
            />
           <!-- android:text="登录密码忘了，提示“账号或密码错误”怎么办？"-->

       <View
           android:id="@+id/faq_result_item_line"
           android:layout_width="match_parent"
           android:layout_height="@dimen/line_h"
           android:layout_below="@id/faq_result_tv"
           android:background="@color/base_line_color"/>


</RelativeLayout>
